    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "stripe": "^12.18.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.3.3"
  }
}
