# Example environment variables for WebsiteOS
# Copy this file to .env and update values as needed

# --- Backend ---
API_PORT=4000
DB_URL=postgres://user:password@localhost:5432/mydb

# --- Auth ---
JWT_SECRET=changeme
SESSION_SECRET=changeme

# --- External APIs ---
API_KEY=changeme

# --- Ops Cycle ---
OPS_MODE=development
OPS_LOG_LEVEL=info

# --- Frontend ---
NEXT_PUBLIC_API_URL=http://localhost:4000