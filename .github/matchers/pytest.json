{
  "problemMatcher": [
    {
      "owner": "pytest",
      "pattern": [
        {
          "regexp": "^E\\s+(?<msg>.+)$",
          "message": "msg",
          "annotationLevel": "failure"
        },
        {
          "regexp": "^(?<file>.+?):(?<line>\\d+): (?<msg>.+)$",
          "file": "file",
          "line": "line",
          "message": "msg",
          "annotationLevel": "failure"
        }
      ]
    }
  ]
}