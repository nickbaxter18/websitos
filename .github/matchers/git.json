{
  "problemMatcher": [
    {
      "owner": "git",
      "pattern": [
        {
          "regexp": ".*exit code 128.*",
          "message": "Git returned exit code 128 (likely no-op fetch/push)",
          "annotationLevel": "warning"
        },
        {
          "regexp": "fatal: (?<msg>.*)$",
          "message": "Git failure: ${msg}",
          "annotationLevel": "failure"
        }
      ]
    }
  ]
}