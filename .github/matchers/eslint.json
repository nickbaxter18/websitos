{
  "problemMatcher": [
    {
      "owner": "eslint",
      "pattern": [
        {
          "regexp": "^(?<file>.*?):(?<line>\\d+):(?<col>\\d+): (?<msg>.*) \\\((?<sev>error|warning).+\\\)$",
          "file": "file",
          "line": "line",
          "column": "col",
          "message": "msg",
          "annotationLevel": "warning"
        }
      ]
    }
  ]
}