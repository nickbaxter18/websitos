{
  "problemMatcher": [
    {
      "owner": "tsc",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+) - error (TS[0-9]+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": "TS Error ($4): $5"
        },
        {
          "regexp": "^(.*)\\((\\d+),(\\d+)\\): error (TS[0-9]+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": "TS Error ($4): $5"
        }
      ]
    }
  ]
}