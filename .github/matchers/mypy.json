{
  "problemMatcher": [
    {
      "owner": "mypy",
      "pattern": [
        {
          "regexp": "^(?<file>.*?):(?<line>\\d+):\\s+(?<sev>error|note):\\s+(?<msg>.+)$",
          "file": "file",
          "line": "line",
          "message": "msg",
          "annotationLevel": "failure"
        }
      ]
    }
  ]
}