{
  "problemMatcher": [
    {
      "owner": "jest",
      "pattern": [
        {
          "regexp": "^FAIL\\s+(?<file>.+)$",
          "file": "file",
          "annotationLevel": "failure"
        },
        {
          "regexp": "^\\s+‚óè\\s+(?<msg>.+)$",
          "message": "msg",
          "annotationLevel": "failure"
        },
        {
          "regexp": "^\\s+at\\s+(?<file>[^:]+):(?<line>\\d+):(?<col>\\d+)$",
          "file": "file",
          "line": "line",
          "column": "col",
          "annotationLevel": "failure"
        }
      ]
    }
  ]
}