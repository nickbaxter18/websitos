{
  "problemMatcher": [
    {
      "owner": "python-traceback",
      "pattern": [
        {
          "regexp": "File \\\"(.+?)\\\", line (\\d+).*",
          "file": 1,
          "line": 2
        }
      ]
    },
    {
      "owner": "python-error",
      "pattern": [
        {
          "regexp": "^(.*Error.*)$",
          "message": 1
        }
      ]
    }
  ]
}