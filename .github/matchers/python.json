{
  "problemMatcher": [
    {
      "owner": "python",
      "pattern": [
        {
          "regexp": "File \\\"(?<file>.+?)\\\", line (?<line>\\d+).*",
          "file": "file",
          "line": "line",
          "message": "Traceback frame",
          "loop": true
        },
        {
          "regexp": "^(?<msg>.*Error.*)$",
          "message": "msg",
          "annotationLevel": "failure"
        }
      ]
    }
  ]
}