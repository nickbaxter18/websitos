{
  "problemMatcher": [
    {
      "owner": "python",
      "pattern": [
        {
          "regexp": "^File \\\"(.+?)\\\", line (\\d+).*$",
          "file": 1,
          "line": 2,
          "message": "Python traceback"
        },
        {
          "regexp": "^(.*Error.*|.*Exception.*)$",
          "message": 1
        }
      ]
    }
  ]
}