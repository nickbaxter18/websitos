{
  "problemMatcher": [
    {
      "owner": "python",
      "pattern": [
        {
          "regexp": "File \\\"(.+?)\\\", line (\\d+).*",
          "file": 1,
          "line": 2,
          "message": "Traceback frame"
        },
        {
          "regexp": "^(.*Error.*)$",
          "message": 1
        }
      ]
    }
  ]
}