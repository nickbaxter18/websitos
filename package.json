{
  "name": "website-os-v2",
  "version": "1.0.2",
  "private": true,
  "description": "WebsiteOS v2 â€“ Unified frontend, backend, and editor stack",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:ci": "jest --passWithNoTests --coverage --coverageReporters=json-summary --coverageReporters=lcov --json --outputFile=jest-report.json",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "ts-node v2/core/schema-validator.ts",
    "validate-env": "ts-node src/validateEnv.ts",
    "verify-assets": "node scripts/verifyAssets.js",
    "prepare": "husky",
    "start": "node server.js",
    "editor": "node server.js",
    "frontend": "vite",
    "backend": "uvicorn api:app --reload",
    "reload": "ts-node reload-update.ts",
    "reload:watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"npm run reload\"",
    "dev:all": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run editor\" \"npm run reload:watch\"",
    "opscycle": "ts-node -P tsconfig.ops.json scripts/opsCycle.ts",
    "secscan": "ts-node -T -P tsconfig.ops.json scripts/scan-secrets.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tanstack/react-query": "^5.0.0",
    "ajv": "8.17.1",
    "ajv-formats": "2.1.1",
    "better-sqlite3": "11.10.0",
    "body-parser": "2.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "2.8.5",
    "dotenv": "^16.4.5",
    "express": "5.1.0",
    "fast-deep-equal": "^3.1.3",
    "fast-uri": "3.1.0",
    "framer-motion": "^12.23.12",
    "helmet": "7.2.0",
    "json-schema-traverse": "1.0.0",
    "lucide-react": "^0.344.0",
    "morgan": "1.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-router-dom": "^6.22.3",
    "re2": "1.22.1",
    "recharts": "3.2.0",
    "require-from-string": "2.0.2",
    "semver": "^7.6.2",
    "stripe": "18.5.0",
    "tailwind-variants": "^0.1.20",
    "zod": "^3.22.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@playwright/test": "^1.44.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.0.0",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "30.1.2",
    "concurrently": "^9.0.0",
    "esbuild": "0.25.9",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "30.1.2",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "1.0.7",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vite-node": "3.2.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,yml,yaml,html}": [
      "prettier --write --allow-empty",
      "eslint --fix --allow-empty"
    ]
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickbaxter18/websitos.git"
  },
  "keywords": [],
