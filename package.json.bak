        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-tailwindcss": "^3.14.0",
        "husky": "8.0.3",
        "jest": "^29.5.0",
        "lint-staged": "^16.1.6",
        "nodemon": "^3.1.0",
        "postcss": "^8.4.32",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.0",
        "tailwindcss": "^3.3.3",
        "tailwindcss-animate": "1.0.7",
        "ts-jest": "^29.4.1",
        "ts-node": "10.9.2",
        "typescript": "5.9.2",
        "vite": "^5.2.0",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.5.0"
    },
    "lint-staged": {
