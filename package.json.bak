{
    "name": "website-os-v2",
    "version": "1.0.0",
    "private": true,
    "description": "WebsiteOS v2 Ã¢â‚¬â€œ Unified frontend, backend, and editor stack",
    "main": "server.js",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "jest",
        "type-check": "tsc --noEmit",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint . --ext .ts,.tsx --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "validate": "ts-node v2/core/schema-validator.ts",
        "prepare": "husky install",
        "start": "node server.js",
        "editor": "node server.js",
        "frontend": "vite",
        "backend": "uvicorn api:app --reload",
        "reload": "ts-node reload-update.ts",
        "reload:watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"npm run reload\"",
        "dev:all": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run editor\" \"npm run reload:watch\"",
        "opscycle": "ts-node -P tsconfig.ops.json scripts/opsCycle.ts",
        "secscan": "ts-node -T -P tsconfig.ops.json scripts/scan-secrets.ts"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.4",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-toast": "^1.1.5",
