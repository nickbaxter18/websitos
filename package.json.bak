        "react-router-dom": "^6.22.3",
        "shadcn-ui": "^0.8.0",
        "tailwind-variants": "^0.1.20",
        "zod": "^3.22.0",
        "zustand": "^4.5.2"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.4",
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/react": "^14.2.1",
        "@types/jest": "^29.5.14",
